	copyright = {2020 This is a U.S. government work and not under copyright protection in the U.S.; foreign copyright protection may apply},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-020-18300-3},
	doi = {10.1038/s41467-020-18300-3},
	abstract = {Climate change is impacting fisheries worldwide with uncertain outcomes for food and nutritional security. Using management strategy evaluations for key US fisheries in the eastern Bering Sea we find that Ecosystem Based Fisheries Management (EBFM) measures forestall future declines under climate change over non-EBFM approaches. Yet, benefits are species-specific and decrease markedly after 2050. Under high-baseline carbon emission scenarios (RCP 8.5), end-of-century (2075–2100) pollock and Pacific cod fisheries collapse in {\textgreater}70\% and {\textgreater}35\% of all simulations, respectively. Our analysis suggests that 2.1–2.3 °C (modeled summer bottom temperature) is a tipping point of rapid decline in gadid biomass and catch. Multiyear stanzas above 2.1 °C become commonplace in projections from {\textasciitilde}2030 onward, with higher agreement under RCP 8.5 than simulations with moderate carbon mitigation (i.e., RCP 4.5). We find that EBFM ameliorates climate change impacts on fisheries in the near-term, but long-term EBFM benefits are limited by the magnitude of anticipated change.},
	language = {en},
	number = {1},
	urldate = {2020-09-15},
	journal = {Nature Communications},
	author = {Holsman, K. K. and Haynie, A. C. and Hollowed, A. B. and Reum, J. C. P. and Aydin, K. and Hermann, A. J. and Cheng, W. and Faig, A. and Ianelli, J. N. and Kearney, K. A. and Punt, A. E.},
	month = sep,
	year = {2020},
	note = {Number: 1
Publisher: Nature Publishing Group},
	pages = {4579},
	file = {Full Text PDF:/Users/sarah.gaichas/Zotero/storage/CDC43GD9/Holsman et al. - 2020 - Ecosystem-based fisheries management forestalls cl.pdf:application/pdf;Full Text PDF:/Users/sarah.gaichas/Zotero/storage/TF5X5H9J/Holsman et al. - 2020 - Ecosystem-based fisheries management forestalls cl.pdf:application/pdf;Snapshot:/Users/sarah.gaichas/Zotero/storage/USFX677D/s41467-020-18300-3.html:text/html;Snapshot:/Users/sarah.gaichas/Zotero/storage/6U5SVABT/s41467-020-18300-3.html:text/html},
