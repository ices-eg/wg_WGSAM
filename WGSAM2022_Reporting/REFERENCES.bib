	month = feb,
	year = {2009},
	keywords = {Goodness-of-fit, Model uncertainty, Skill assessment, Skill metric},
	pages = {4--15},
	file = {Accepted Version:/Users/sarah.gaichas/Zotero/storage/374XQ63Y/Stow et al. - 2009 - Skill assessment for coupled biologicalphysical m.pdf:application/pdf;ScienceDirect Snapshot:/Users/sarah.gaichas/Zotero/storage/CZZUNMXD/S0924796308001103.html:text/html;ScienceDirect Snapshot:/Users/sarah.gaichas/Zotero/storage/8U8EF3FD/S0924796308001103.html:text/html},
